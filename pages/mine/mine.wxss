page {width: 100%;height: 100%;background: #f5f5f5;}
.header-con {float: left;width: 750rpx;height: 236rpx;background-color: #4d95e9;}
.figure-con {width: 100%;height: 194rpx;}
.figure-img {width: 140rpx;height: 140rpx;border-radius: 50%;top: 25rpx;}
.figure-txt {width: 100%;text-align: center;bottom: 19rpx;font-family: 'PingFang-SC-Medium';font-weight: normal;font-stretch: normal;letter-spacing: 0px;}
.list-con {width: 710rpx;margin: 0 auto;min-height: 1rpx;}
